 {
    "name": "Fake News Detector",
    "version": "1.0",
    "description": "Detect Fake News",
    "permissions": ["activeTab", "http://localhost/*"],
    "manifest_version": 2,
    "background": {
    	"scripts": ["js/jquery-3.5.1.min.js", "js/background.js"],
    	"persistent": false
    },
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": ["js/content.js"]
		}
	],
	"browser_action": {
		"default_popup": "html/popup.html"
	}
}